{
    "name": "mautic/embed-url-option-bundle",
    "description": "Adds a 'Direct URL' button to Mautic form detail pages for easy access to form URLs",
    "type": "mautic-plugin",
    "license": "GPL-3.0-or-later",
    "authors": [
        {
            "name": "Frederik Wouters",
            "email": "woutersf@gmail.com"
        }
    ],
    "keywords": [
        "mautic",
        "plugin",
        "form",
        "url",
        "embed",
        "direct-url"
    ],
    "require": {
        "php": "^7.4|^8.0",
        "mautic/core": "^4.0|^5.0"
    },
    "autoload": {
        "psr-4": {
            "MauticPlugin\\MauticEmbedUrlOptionBundle\\": ""
        }
    },
    "extra": {
        "install-directory-name": "MauticEmbedUrlOptionBundle"
    }
}
